    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "embla-carousel-react": "^8.3.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "framer-motion": "^11.13.1",
    "http-proxy-middleware": "^2.0.6",
    "input-otp": "^1.2.4",
    "kill-port": "^2.0.1",
    "lucide-react": "^0.453.0",
    "msw": "^2.7.5",
