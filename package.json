{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env -- npx tsx server/index.ts",
    "dev:test": "NODE_ENV=test E2E_TEST_MODE=true LOG_LEVEL=info PORT=5001 dotenv -e .env.test -- npx tsx server/index.ts",
    "dev:client": "dotenv -e .env -- vite --config config/vite.config.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --config config/vite.config.ts",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "check": "tsc",
    "db:migrations:generate": "drizzle-kit generate --config=./config/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=config/drizzle.config.ts",
    "db:push:test": "dotenv -e .env.test -- drizzle-kit push --config=config/drizzle.config.ts",
    "db:migrations:apply": "npx tsx ./db-migration-scripts/migrate.ts",
    "db:migrations:apply:test": "NODE_ENV=test npx tsx ./db-migration-scripts/migrate.ts",
    "db:migrations:check:test": "NODE_ENV=test npx tsx ./db-migration-scripts/check-migrations.ts",
    "db:audit": "npx tsx ./scripts/audit-database-integrity.ts",
    "db:audit:test": "NODE_ENV=test npx tsx ./scripts/audit-database-integrity.ts",
    "db:emergency:introspect": "drizzle-kit introspect --config=config/drizzle.config.ts",
    "db:emergency:introspect:test": "NODE_ENV=test drizzle-kit introspect --config=config/drizzle.config.ts",
    "db:emergency:push": "drizzle-kit push --config=config/drizzle.config.ts",
    "db:emergency:push:test": "NODE_ENV=test drizzle-kit push --config=config/drizzle.config.ts",
    "db:reset": "npx tsx ./db-migration-scripts/reset-db.ts",
    "db:reset:test": "npx tsx ./db-migration-scripts/reset-db-test.ts",
    "db-test:reset": "npx tsx ./db-migration-scripts/reset-db-test.ts",
    "test": "NODE_ENV=test vitest --config=test-config/vitest/vitest.unified.config.mjs",
    "test:unit": "NODE_ENV=test TEST_MODE=unit vitest run tests/unit --config=test-config/vitest/vitest.unified.config.mjs",
    "test:integration": "NODE_ENV=test TEST_MODE=integration vitest run tests/integration --config test-config/vitest/vitest.unified.config.mjs",
    "test:integration:watch": "NODE_ENV=test TEST_MODE=integration vitest --config test-config/vitest/vitest.unified.config.mjs",
    "test:coverage": "NODE_ENV=test vitest --config=test-config/vitest/vitest.unified.config.mjs --coverage",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:e2e": "NODE_ENV=test E2E_TEST_MODE=true playwright test --config=test-config/playwright.config.ts",
    "test:e2e:ui": "NODE_ENV=test E2E_TEST_MODE=true playwright test --config=test-config/playwright.config.ts --ui",
    "test:e2e:debug": "NODE_ENV=test E2E_TEST_MODE=true playwright test --config=test-config/playwright.config.ts --debug"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@babel/preset-env": "^7.26.9",
