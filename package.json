  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "NODE_ENV=test vitest --config=test-config/vitest/vitest.unified.config.mjs",
    "test:unit": "NODE_ENV=test TEST_MODE=unit vitest --config=test-config/vitest/vitest.unified.config.mjs",
    "test:integration": "vitest run --config ./test-config/vitest/vitest.integration.config.mjs",
    "test:integration:watch": "vitest --config ./test-config/vitest/vitest.integration.config.mjs",
    "test:coverage": "NODE_ENV=test vitest --config=test-config/vitest/vitest.unified.config.mjs --coverage",
    "test:analyze-redundancy": "tsx tests/analyze-test-redundancy.ts",
    "test:cleanup-redundancy": "tsx tests/cleanup-redundant-tests.ts",
    "test:all": "npm run test:unit && npm run test:integration"
  },
